   "source": [
    "%sql\n",
    "--for todays data\n",
    "SELECT\n",
    "    usage_date,\n",
    "    SUM(usage_quantity * list_price_in_usd) AS total_daily_usage_usd\n",
    "FROM\n",
    "    system.billing.usage\n",
    "WHERE\n",
    "    usage_date IN (DATE('2025-04-26'), DATE('2025-04-25'))\n",
    "GROUP BY\n",
    "    usage_date\n",
    "ORDER BY\n",
    "    usage_date DESC;"
   ]
  }
